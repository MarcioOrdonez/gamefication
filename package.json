{
  "name": "gamefication",
  "version": "1.0.0",
  "description": "Projeto de gameficação",
  "scripts": {
    "client": "cd client && npm start",
    "server": "cd backend && node index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client-dependencies": "cd client && npm install",
    "server-dependencies": "cd backend && npm install",
    "install-dependencies": "concurrently --kill-others-on-fail \"npm run server-dependencies\" \"npm run client-dependencies\""
  },
